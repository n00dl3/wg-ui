{
  "name": "wireguard-ui",
  "version": "1.0.0",
  "description": "WireGuard VPN web UI",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json && tsc -p tsconfig.node.json",
    "prepare": "cd .. && husky ui/.husky"
  },
  "devDependencies": {
    "@material/typography": "^14.0.0",
    "@smui/button": "^7.0.0",
    "@smui/dialog": "^7.0.0",
    "@smui/fab": "^7.0.0",
    "@smui/form-field": "^7.0.0",
    "@smui/icon-button": "^7.0.0",
    "@smui/paper": "^7.0.0",
    "@smui/switch": "^7.0.0",
    "@smui/textfield": "^7.0.0",
    "@smui/top-app-bar": "^7.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tsconfig/svelte": "^5.0.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "sass-embedded": "^1.77.8",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.1",
    "svelte-routing": "^2.13.0",
    "tslib": "^2.6.3",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  },
  "dependencies": {
    "cookie-universal": "^2.2.2",
    "svelte-material-ui": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md,ts,svelte}": [
      "prettier --write",
      "git add"
    ]
  }
}
